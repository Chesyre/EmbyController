{
  "kind": "collectionType",
  "collectionName": "devices",
  "info": {
    "singularName": "device",
    "pluralName": "devices",
    "displayName": "Device",
    "description": "Appareils Emby connect√©s"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "deviceId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "client": {
      "type": "string",
      "required": true
    },
    "lastUsedAt": {
      "type": "datetime"
    },
    "lastIp": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive",
        "blocked"
      ],
      "default": "active"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "devices"
    },
    "metadata": {
      "type": "json"
    }
  }
}